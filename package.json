{
	"name": "websheets-bio-link",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"test": "jest",
		"test:e2e": "yarn test:e2e:run",
		"test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
		"test:e2e:open": "start-server-and-test start http://localhost:3000 cy:open",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"lint": "eslint  --ignore-path .gitignore --ext .js,.ts,tsx .",
		"prettier": "prettier --ignore-path .gitignore  \"**/*.+(js|ts|tsx|json|css)\"",
		"format": "yarn prettier --write",
		"check-format": "prettier --list-different",
		"check-types": "tsc",
		"validate": "npm-run-all --parallel check-types lint test test:e2e build",
		"setup": "yarn install && yarn validate"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"identity-obj-proxy": "^3.0.0",
		"next": "10.0.7",
		"react": "17.0.1",
		"react-dom": "17.0.1"
	},
	"devDependencies": {
		"@testing-library/cypress": "^7.0.3",
		"@testing-library/dom": "^7.29.4",
		"@testing-library/jest-dom": "^5.11.9",
		"@testing-library/react": "^11.2.5",
		"@types/node": "^14.14.28",
		"@types/react": "^17.0.2",
		"@typescript-eslint/eslint-plugin": "^4.15.1",
		"@typescript-eslint/parser": "^4.15.1",
		"autoprefixer": "^10.2.4",
		"babel-jest": "^26.6.3",
		"cypress": "^6.5.0",
		"eslint": "^7.20.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-cypress": "^2.11.2",
		"husky": "4.2.3",
		"jest": "^26.6.3",
		"jest-dom": "^4.0.0",
		"lint-staged": "^10.5.4",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.6",
		"prettier": "^2.2.1",
		"start-server-and-test": "^1.12.0",
		"tailwindcss": "^2.0.3",
		"typescript": "^4.1.5"
	}
}
